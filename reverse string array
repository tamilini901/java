class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        String s="I love Germany";
        String[] arr =  s.split(" ");
        String ss = "";
        int n=arr.length;
        for(int i=n-1;i>=0;i--){
            ss+=arr[i]+" ";
        }
        System.out.println(ss);
       
    }
}
