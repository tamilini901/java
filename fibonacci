import java.util.*;
class HelloWorld {
    
     public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a integer: ");
        int a=sc.nextInt();
          for (int i = 0; i < a; i++) {
            System.out.print(fibo(i) + " ");
        }
        sc.close();
    }
    public static int fibo(int a){
      
       if(a<=1){
           return a;
       }
       return fibo(a-1)+fibo(a-2);
    
}
}
