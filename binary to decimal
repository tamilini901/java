class Main {
    public static void main(String[] args) {
        String str = "1001";
        double ans = 0;
        int p = 0;
        
        for (int i = str.length() - 1; i >= 0; i--) {
            if (str.charAt(i) == '1') {
                ans = ans+Math.pow(2, p);
            }
             p++;
           
        }
        
        System.out.println((int) ans);
    }
}
